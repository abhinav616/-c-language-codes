
#include <stdio.h>

int main() {
    int a, b, temp;
   printf("enter a number for a ");
   scanf("%d",&a);
   
   printf("enter a number foR b");
   scanf("%d", &b);
   
   printf("before swapping a is %d and b is %d\n",a,b);
   temp = a;
   a=b;
   b=temp;
   printf("after swapping a is %d and b is %d\n ",a,b);
   
    return 0;
    
}
