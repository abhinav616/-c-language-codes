
#include <stdio.h>

int main() {
    
  
   int num1=23;
   int num2=23;
   int sum;
   sum=num1+num2;
    
    printf("The sum of %d and %d is %d\n", num1, num2, sum);
    int sub;
    sub=num1-num2;
    printf("The subtraction of %d and %d is %d\n", num1, num2, sum);
    
    int prod;
    prod=num1*num2;
    printf("The product of %d and %d is %d\n", num1, num2, sum);
    
    int div;
    div=num1/num2;
    printf("The division of %d and %d is %d\n", num1, num2, sum);
    
    
    return 0;
    
}
